{
  "manifest_version": 2,
  "name": "Github Change",
  "version": "1.0",
  "description": "This extensions alters the look and content of a github page.",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*","<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
      "images/rainbow.png",
      "images/yoga.png"
  ]
}
